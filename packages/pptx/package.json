{
  "name": "@opendockit/pptx",
  "version": "0.0.1",
  "description": "Progressive-fidelity PPTX renderer (SlideKit)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@opendockit/core": "workspace:*"
  },
  "devDependencies": {
    "jszip": "^3.10.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
